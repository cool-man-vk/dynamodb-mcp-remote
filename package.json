{
  "name": "dynamodb-mcp-server",
  "version": "0.2.0",
  "description": "DynamoDB MCP server for managing DynamoDB resources via remote HTTP/SSE transport",
  "author": "Vignesh Kumar(rrvigneshkumar2002@gmail.com",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "mcp-server-dynamodb": "dist/index.js"
  },
  "files": [
    "dist",
    "smithery.json"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "smithery": "node smithery-dev.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.723.0",
    "@aws-sdk/util-dynamodb": "^3.723.0",
    "@modelcontextprotocol/sdk": "0.5.0"
  },
  "devDependencies": {
    "@smithery/cli": "^1.6.3",
    "@types/node": "^22.10.5",
    "dotenv": "^16.4.5",
    "shx": "^0.3.4",
    "typescript": "^5.6.2"
  }
}
